{
  "name": "nest-prisma-base",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test -- npx prisma migrate reset --force && dotenv -e .env.test -- jest --runInBand --config ./test/jest-e2e.json --forceExit --detectOpenHandles",
    "test:e2e:watch": "dotenv -e .env.test -- jest --runInBand --config ./test/jest-e2e.json --forceExit --detectOpenHandles --watch",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:nofix": "eslint \"{src,apps,libs,test}/**/*.ts\" ",
    "lint:inspect": "eslint --inspect-config",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --dry-run",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset",
    "db:refresh": "prisma migrate reset && prisma db seed",
    "db:studio:test": "prisma studio --env-file .env.test",
    "db:studio:prod": "prisma studio --env-file .env.prod",
    "db:studio:dev": "prisma studio --env-file .env.dev",
    "db:studio:local": "prisma studio --env-file .env.local"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^8.1.1",
    "@nestjs/throttler": "^6.3.0",
    "@prisma/client": "^6.2.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "multer": "1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^9.3.0",
    "@golevelup/ts-jest": "^0.6.1",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@stylistic/eslint-plugin-ts": "^2.12.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.3.0",
    "node-mocks-http": "^1.16.2",
    "prettier": "^3.4.2",
    "prisma": "^6.2.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t)s$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "globals": {}
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "pnpm format",
      "pnpm lint"
    ]
  }
}
